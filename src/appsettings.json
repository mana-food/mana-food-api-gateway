{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Secret": "${JWT_SECRET}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}"
  },
  "Services": {
    "UserService": {
      "Url": "${USER_SERVICE_URL}"
    },
    "AuthLambda": {
      "Url": "${AUTH_LAMBDA_URL}"
    },
    "PaymentService": {
      "Url": "${PAYMENT_SERVICE_URL}"
    }
  }
}